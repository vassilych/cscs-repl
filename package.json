{
    "name": "replcscs",
    "displayName": "replCSCS",
    "description": "The REPL protocol for the CSCS Debugging",
    "version": "0.0.1",
    "publisher": "vassilik",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.cscs.sendToREPL",
                "title": "Send to REPL"
            }
        ],
        "keybindings": [{
            "command": "extension.cscs.sendToREPL",
            "key": "ctrl+9",
            "mac": "cmd+'Â¨",
            "when": "editorTextFocus"
        }],
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
